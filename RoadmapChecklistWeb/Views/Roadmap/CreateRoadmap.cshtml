@model Service.Roadmaps.Roadmaps.Models.RoadmapViewModel
@{
    ViewData["Title"] = "CreateRoadmap";
}

<h1>Roadmap</h1>
<form class="login100-form validate-form p-b-33 p-t-5" asp-controller="Roadmap" asp-action="AddRoadmap" method="post" role="form">
    <ul>
        <li>
            <div>
                <input asp-for="Name" placeholder="Name" />
                <span asp-validation-for="Name"></span>
            </div>
        </li>
        <li>
            <div>
                <input asp-for="UserId" placeholder="UserId" />
                <span asp-validation-for="UserId"></span>
            </div>
        </li>
        <li>
            <div>
                <input asp-for="Visibility" placeholder="Visibility" />
                <span asp-validation-for="Visibility" p></span>
            </div>
        </li>
        <li>
            <div>
                <input asp-for="StartDate" placeholder="StartDate" />
                <span asp-validation-for="StartDate"></span>
            </div>
        </li>
        <li>
            <div>
                <input asp-for="EndDate" placeholder="EndDate" />
                <span asp-validation-for="EndDate"></span>
            </div>
        </li>
        <li>
            <div>
                <input asp-for="Status" placeholder="Status" />
                <span asp-validation-for="Status"></span>
            </div>
        </li>
    </ul>
    <div class="container-login100-form-btn m-t-32">
        <button type="submit" class="login100-form-btn">
            Kaydet
        </button>
    </div>
    <div class="container-login100-form-btn m-t-32">
        <a href="@Url.Action("RoadmapList","Roadmap")">
            Roadmap Liste
        </a>
    </div>
</form>


